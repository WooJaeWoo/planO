    <description></description>
    <display-name>getProjectInfo</display-name>
    <servlet-name>getProjectInfo</servlet-name>
    <servlet-class>controller.getProjectInfo</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>getProjectInfo</servlet-name>
    <url-pattern>/getProjectInfo</url-pattern>
