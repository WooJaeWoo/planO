  </servlet>
  <servlet-mapping>
    <servlet-name>GetProjectInfoController</servlet-name>
    <url-pattern>/GetProjectInfoController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>WebServerLauncher</display-name>
    <servlet-name>WebServerLauncher</servlet-name>
    <servlet-class>WebServerLauncher</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>WebServerLauncher</servlet-name>
    <url-pattern>/WebServerLauncher</url-pattern>
  </servlet-mapping>
</web-app>